                                <ui:Label tabindex="-1" text="0" display-tooltip-when-elided="true" class="text" style="visibility: hidden;" />
                                <ui:Label tabindex="-1" text="0" display-tooltip-when-elided="true" class="text" style="visibility: hidden;" />
