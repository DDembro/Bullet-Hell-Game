            <ui:Label tabindex="-1" text="Shop" display-tooltip-when-elided="true" name="level-selector-title" enable-rich-text="true" class="headerText" style="display: flex; font-size: 70%; -unity-font: initial; -unity-font-style: bold; -unity-text-align: upper-center; margin-left: 40px; margin-right: 40px; margin-top: 40px; margin-bottom: 70px; align-self: center;" />
