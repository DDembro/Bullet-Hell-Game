                <ui:Button text="Button" display-tooltip-when-elided="true" class="level-button" style="display: flex; visibility: hidden;" />
                <ui:Button text="Button" display-tooltip-when-elided="true" class="level-button" style="display: flex; visibility: hidden;" />
                <ui:Button text="Button" display-tooltip-when-elided="true" class="level-button" style="visibility: hidden;" />
                <ui:Button text="Button" display-tooltip-when-elided="true" class="level-button" style="visibility: hidden;" />
                <ui:Button text="Button" display-tooltip-when-elided="true" class="level-button" style="visibility: hidden;" />
                <ui:Button text="Button" display-tooltip-when-elided="true" class="level-button" style="visibility: hidden;" />
            </ui:VisualElement>
            <ui:VisualElement name="BottomBox" class="container" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row;">
