<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/USS/LevelSelectorStyles.uss?fileID=7433441132597879392&amp;guid=d42d77e81ee660f4cb494003849cdc96&amp;type=3#LevelSelectorStyles" />
    <ui:VisualElement class="container" style="flex-grow: 1; background-color: rgb(40, 59, 77);">
        <ui:VisualElement name="LevelSelectorView" class="container" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); display: flex;">
